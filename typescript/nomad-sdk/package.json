{
  "name": "@nomad-xyz/sdk",
  "version": "0.1.11",
  "description": "Nomad SDK",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist/ && rm tsconfig.tsbuildinfo & tsc && cp README.md ./dist/ && cp package.json ./dist/",
    "check": "rm tsconfig.tsbuildinfo & tsc --noEmit",
    "prettier": "prettier --write ./src",
    "lint": "npm run prettier && eslint ./src",
    "lint:fix": "npm run prettier && eslint --fix ./src",
    "publish-npm": "npm run build && cd ./dist/ && npm publish && cd ../"
  },
  "author": "Illusory Systems Inc.",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "fs": "0.0.1-security",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@nomad-xyz/contract-interfaces": "file:../typechain",
    "ethers": "^5.4.6"
  },
  "keywords": [
    "nomad",
    "sdk",
    "provider",
    "multi-provider"
  ]
}
